<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <EditText
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:hint="@string/nama_lokasi"
        android:textSize="25dp"
        android:textAlignment="center"
        android:gravity="center"/>
    <EditText
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:textSize="20sp"
        android:hint="@string/nama_kota"
        android:gravity="center"
        android:layout_gravity="center"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:orientation="horizontal"
        android:weightSum="1">
        <TextView
            android:id="@+id/longitude"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:background="#345"
            android:text="X : 0.1234456789"
            android:textColor="#fff"
            android:textSize="20sp"
            android:gravity="center"/>
        <TextView
            android:id="@+id/latitude"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:background="#345"
            android:text="Y : 0.1234456789"
            android:textColor="#fff"
            android:textSize="20sp"
            android:gravity="center"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="@string/analisa"
        android:gravity="center"
        android:textSize="30dp"
        android:background="@color/counter_text_color"/>
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="320dp"
        android:id="@+id/scrollView1">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/counter_text_bg"
        android:stretchColumns="1">
        <TableRow
            android:background="@color/counter_text_color"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="5dip">
            <TextView
                android:text="@string/bau"
                android:paddingLeft="30dip"
                android:paddingRight="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center|left"/>
            <Spinner
                android:id="@+id/bau"
                android:gravity="right"
                android:textSize="24dp"
                android:entries="@array/baulist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="5dip">
            <TextView
                android:text="@string/warna"
                android:paddingLeft="30dip"
                android:paddingRight="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center|left"/>
            <Spinner
                android:id="@+id/warna"
                android:entries="@array/warnalist"
                android:gravity="right"
                android:paddingRight="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="5dip">
            <TextView
                android:text="@string/rasa"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center|left"/>
            <Spinner
                android:id="@+id/rasa"
                android:entries="@array/rasalist"
                android:gravity="right"
                android:paddingRight="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:padding="5dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/dhl"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <EditText
                        android:id="@+id/dhl"
                        android:inputType="numberDecimal"
                        android:gravity="right"
                        android:paddingRight="10dip"
                        android:textSize="24dp"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="10"
                        android:maxWidth="100dp"/>
                    <TextView
                        android:text="@string/DHL"
                        android:paddingRight="30dp"
                        android:textSize="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>
            </TableLayout>
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:padding="5dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/ph"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <EditText
                android:id="@+id/ph"
                android:inputType="numberDecimal"
                android:gravity="right"
                android:paddingRight="30dip"
                android:textSize="24dp"
                android:maxLines="10"
                android:maxWidth="120dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:padding="5dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/tds"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <EditText
                        android:id="@+id/tds"
                        android:inputType="numberDecimal"
                        android:gravity="right"
                        android:paddingRight="10dip"
                        android:textSize="24dp"
                        android:maxLines="10"
                        android:maxWidth="120dp"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/TDS"
                        android:paddingRight="30dp"
                        android:textSize="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>
            </TableLayout>
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:padding="5dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/fe"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <EditText
                        android:id="@+id/fe"
                        android:inputType="numberDecimal"
                        android:gravity="right"
                        android:paddingRight="10dip"
                        android:textSize="24dp"
                        android:maxLines="10"
                        android:maxWidth="120dp"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/TDS"
                        android:paddingRight="30dp"
                        android:textSize="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>
            </TableLayout>
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:padding="5dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/no2"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <EditText
                        android:id="@+id/no2"
                        android:inputType="numberDecimal"
                        android:gravity="right"
                        android:paddingRight="10dip"
                        android:textSize="24dp"
                        android:maxLines="10"
                        android:maxWidth="120dp"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/TDS"
                        android:paddingRight="30dp"
                        android:textSize="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>
            </TableLayout>
        </TableRow>
        <TableRow
            android:background="@color/counter_text_color"
            android:padding="5dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="@string/f"
                android:paddingLeft="30dip"
                android:textSize="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <EditText
                        android:id="@+id/f"
                        android:inputType="numberDecimal"
                        android:gravity="right"
                        android:paddingRight="10dip"
                        android:textSize="24dp"
                        android:maxLines="10"
                        android:maxWidth="120dp"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/TDS"
                        android:paddingRight="30dp"
                        android:textSize="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>
            </TableLayout>
        </TableRow>
    </TableLayout>
    </ScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:weightSum="1">
        <Button
            android:id="@+id/cancel"
            android:layout_above="@id/scrollView1"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text="@string/cancel"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textSize="24dp"
            android:layout_weight="0.5"
            android:gravity="center"
            android:background="@color/red"/>
        <Button
            android:id="@+id/save"
            android:layout_above="@id/scrollView1"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text="@string/save"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textSize="24dp"
            android:layout_weight="0.5"
            android:gravity="center"
            android:background="@color/green"/>
    </LinearLayout>
</LinearLayout><!-- From: file:/C:/Users/Arya/AndroidstudioProjects/SlidingMenu1/app/src/main/res/layout/form_add_layout.xml -->